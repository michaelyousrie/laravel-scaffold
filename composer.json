{
    "name": "michael_yousrie/laravel_scaffold",
    "description": "Basic laravel scaffolding for common functionality.",
    "authors": [
        {
            "name": "Michael Yousrie",
            "email": "michaelyousrie@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelScaffold\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelScaffold\\ScaffoldServiceProvider"
            ]
        }
    }
}
